  "type": "module",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "check:format": "prettier --write \"./!(node_modules|dist)/**/*.{ts,tsx,js,jsx}\"",
    "check:typescript": "tsc --noEmit -p .",
    "check:eslint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "check": "npm run check:format && npm run check:typescript && npm run check:eslint",
    "build:digitalocean": "pnpm install --production=false && pnpm run build && rm -rf node_modules && pnpm install --production --frozen-lockfile",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "tsc-files --noEmit"
  },
